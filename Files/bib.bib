@article{fru2013a,
  title = {Long-term wind resource and uncertainty estimation using wind records from Scotland as example},
  language = {English},
  publisher = {Elsevier Science Ltd.},
  author = {Fr\"uh, Wolf-Gerrit},
  journal = {Renewable Energy},
  volume = {50},
  pages = {1014-1026},
  year = {2013},
  issn = {09601481, 18790682},
  abstract = {A systematic analysis of the sensitivity of a wind turbine's output to changes in observed wind statistics between different sites in Scotland over available wind records of up to 43 years length was performed. The analysis was performed in the context of observed variability on time scales longer than a year. The findings are discussed in the context of the ability to predict the long-term wind energy potential reliably both for wind farms as well as small turbines. In the analysis, some measures are defined to quantify the forecast accuracy and the long-term prediction error. One of the items of discussion was motivated by the observation in the wind industry that the year 2010 was a poor year, with hopes that it was just an exceptional year and fears that it might be an indicator of continuing climate change. The result of this discussion is that 2010 can only be seen as an outlier if one assumes that the past decades represent a constant wind climate. A linear regression, however, suggests that this assumption may not be correct and that 2010 may have been a low-wind year but consistent with generally observed fluctuations around a changing wind climate. [All rights reserved Elsevier].}
}
@article{oh2012a,
